<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".AddActivity">

  <EditText
      android:id="@+id/Nombre"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginStart="95dp"
      android:layout_marginTop="95dp"
      android:layout_marginEnd="107dp"
      android:ems="10"
      android:inputType="text"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent" />

  <EditText
      android:id="@+id/Apellidos"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginStart="95dp"
      android:layout_marginTop="30dp"
      android:layout_marginEnd="107dp"
      android:ems="10"
      android:inputType="text"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/Nombre" />

  <TextView
      android:id="@+id/textView"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginStart="98dp"
      android:layout_marginEnd="263dp"
      android:text="Nombre"
      app:layout_constraintBottom_toTopOf="@+id/Nombre"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="parent" />

  <TextView
      android:id="@+id/textView2"
      android:layout_width="59dp"
      android:layout_height="25dp"
      android:layout_marginStart="96dp"
      android:layout_marginEnd="256dp"
      android:text="Apellidos"
      app:layout_constraintBottom_toTopOf="@+id/Apellidos"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="parent" />

  <EditText
      android:id="@+id/Domicilio"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginStart="95dp"
      android:layout_marginTop="30dp"
      android:layout_marginEnd="107dp"
      android:ems="10"
      android:inputType="text"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/Apellidos" />

  <EditText
      android:id="@+id/Correo_Electronico"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginStart="95dp"
      android:layout_marginTop="30dp"
      android:layout_marginEnd="107dp"
      android:ems="10"
      android:inputType="textEmailAddress"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/Domicilio" />

  <TextView
      android:id="@+id/textView3"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginStart="95dp"
      android:layout_marginEnd="258dp"
      android:text="Domicilio"
      app:layout_constraintBottom_toTopOf="@+id/Domicilio"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="parent" />

  <TextView
      android:id="@+id/textView4"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginStart="94dp"
      android:layout_marginEnd="201dp"
      android:text="Correo electronico"
      app:layout_constraintBottom_toTopOf="@+id/Correo_Electronico"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="parent" />

  <EditText
      android:id="@+id/Telefono"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginStart="95dp"
      android:layout_marginTop="30dp"
      android:layout_marginEnd="107dp"
      android:ems="10"
      android:inputType="phone"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/Correo_Electronico" />

  <TextView
      android:id="@+id/textView5"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginStart="94dp"
      android:layout_marginEnd="262dp"
      android:text="Telefono"
      app:layout_constraintBottom_toTopOf="@+id/Telefono"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="parent" />

  <EditText
      android:id="@+id/Edad"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginStart="95dp"
      android:layout_marginTop="30dp"
      android:layout_marginEnd="107dp"
      android:ems="10"
      android:inputType="number"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/Telefono" />

  <TextView
      android:id="@+id/textView6"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginStart="94dp"
      android:layout_marginEnd="286dp"
      android:text="Edad"
      app:layout_constraintBottom_toTopOf="@+id/Edad"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.0"
      app:layout_constraintStart_toStartOf="parent" />

  <Button
      android:id="@+id/reset_button1"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginStart="90dp"
      android:layout_marginTop="25dp"
      android:layout_marginEnd="6dp"
      android:text="Resetear"
      app:layout_constraintEnd_toStartOf="@+id/add_button1"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/Edad" />

  <Button
      android:id="@+id/add_button1"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginStart="6dp"
      android:layout_marginTop="25dp"
      android:layout_marginEnd="101dp"
      android:text="Agregar"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toEndOf="@+id/reset_button1"
      app:layout_constraintTop_toBottomOf="@+id/Edad" />

</androidx.constraintlayout.widget.ConstraintLayout>